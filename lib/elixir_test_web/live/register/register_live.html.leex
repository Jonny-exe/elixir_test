<%= form_for @changeset, "/", [phx_change: "validate", phx_submit: "register"],  fn f -> %>
  <%= text_input f, :name, placeholder: "name" %>
  <%= error_tag f, :name %>
  <%= text_input f, :password, placeholder: "password" %>
  <%= error_tag f, :password %>
  <%= submit "Register", phx_disable_with: "Logging in...", disabled: !@changeset.valid? %>
<% end %>
